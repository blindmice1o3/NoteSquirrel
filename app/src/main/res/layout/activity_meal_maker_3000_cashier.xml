<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00FF00"
    tools:context=".sandbox.mealmaker3000cashier.MealMaker3000CashierActivity">

    <FrameLayout
        android:id="@+id/framelayout_mealmaker3000cashier_meal_staging_screen"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:hint="meal staging screen"
        app:layout_constraintBottom_toTopOf="@id/button_mealmaker3000cashier_send_meal_request"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/framelayout_mealmaker3000cashier_fine_grain_controls"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_mealmaker3000cashier_send_meal_request"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Send Meal Request"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/framelayout_mealmaker3000cashier_fine_grain_controls"
        app:layout_constraintTop_toBottomOf="@id/framelayout_mealmaker3000cashier_meal_staging_screen" />

    <FrameLayout
        android:id="@+id/framelayout_mealmaker3000cashier_fine_grain_controls"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@id/framelayout_mealmaker3000cashier_meal_staging_screen"
        app:layout_constraintRight_toLeftOf="@id/framelayout_mealmaker3000cashier_course_grain_controls"
        app:layout_constraintTop_toTopOf="parent" />

    <FrameLayout
        android:id="@+id/framelayout_mealmaker3000cashier_course_grain_controls"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@id/framelayout_mealmaker3000cashier_fine_grain_controls"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>