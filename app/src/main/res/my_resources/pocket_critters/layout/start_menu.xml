<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/start_menu_linearlayout"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@drawable/border_start_menu"
    android:orientation="vertical">

    <TextView
        android:id="@+id/dialog_critter_dex"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:text="Critterdex"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/dialog_belt_list"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:text="Belt List"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/dialog_backpack_list"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:text="Backpack List"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/dialog_load"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:text="Load"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/dialog_save"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:text="Save"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/dialog_option"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:text="Option"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/dialog_exit"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:text="Exit"
        android:textSize="20sp" />

</LinearLayout>