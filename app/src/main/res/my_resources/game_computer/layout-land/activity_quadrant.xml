<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintlayout_quadrant_land"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".gameboycolor.gamecartridges.derived.pocketcritters.computer.QuadrantActivity">

    <FrameLayout
        android:id="@+id/framelayout_quadrant_01_land"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#FF0000"
        app:layout_constraintBottom_toTopOf="@id/framelayout_quadrant_03_land"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/framelayout_quadrant_02_land"
        app:layout_constraintTop_toTopOf="parent" />

    <FrameLayout
        android:id="@+id/framelayout_quadrant_02_land"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#FFFF00"
        app:layout_constraintBottom_toTopOf="@id/framelayout_quadrant_04_land"
        app:layout_constraintLeft_toRightOf="@id/framelayout_quadrant_01_land"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <FrameLayout
        android:id="@+id/framelayout_quadrant_03_land"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#0000FF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/framelayout_quadrant_04_land"
        app:layout_constraintTop_toBottomOf="@id/framelayout_quadrant_01_land" />

    <FrameLayout
        android:id="@+id/framelayout_quadrant_04_land"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#00FF00"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@id/framelayout_quadrant_03_land"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/framelayout_quadrant_02_land" />

</androidx.constraintlayout.widget.ConstraintLayout>